.b{ font-weight:bold;}
.flex-wrap{flex-wrap: wrap}
.p22{ width:22%;}
.flex-left{ justify-content: flex-start;}
.grey-bg{background:#fafafa; width:100% }
.header-search-bar{ background:url(https://wxapi.goodjobs.cn/images/g-head-bg.png?=113223) no-repeat top center; background-size:100%; height:112rpx; flex-direction:row; flex-wrap:nowrap; align-items:center; justify-content:center;background-position:0 -8rpx;}
.header-search-bar__form{background:#fff; width:690rpx; height: 70rpx; border-radius: 36rpx; box-shadow: 0 0 20rpx #999; display:inline-flex; display:-webkit-inline-flex; justify-content:center; align-items: center; }
.header-search-bar__form .txt{font-size: 30rpx; font-weight: 200; color: #aaa;}
.search-bar-box__input{flex-basis: auto;}
.borderg-t{border:1rpx solid #eee;}

/*列表部分*/
.f-list-content{margin-top: 30rpx;}
.f-list-content .list-item{padding:20rpx 30rpx; border-bottom:1rpx  solid #eee;}
.f-list-content .list-item::after{content:"";overflow:hidden;clear:both;height:0;display: block;}
.f-list-content .company-logo{width:90rpx; height: 90rpx; box-shadow: 0 0 10rpx #ddd; float: left;border-radius:15%;}
.f-list-content .company-logo .photo{width:100%; height:100%;border-radius:15%; }
.f-list-content .right-content{width:580rpx; float:left; margin-left:20rpx; font-size: 26rpx; color:#999;}
.f-list-content .right-content .job-name{display:flex; display:-webkit-flex;}
.f-list-content .right-content .job-name .job-name-view{flex:1;display:flex; display:-webkit-flex;}
.f-list-content .right-content .job-name .job-name-view .title{flex-wrap: nowrap; max-width: 440rpx; overflow: hidden;white-space: nowrap;text-overflow: ellipsis;margin-right:8rpx;}
.f-list-content .right-content .job-name .money{justify-content:right }
.f-list-content .right-content .title{font-size:32rpx; color: #333; margin-bottom:8rpx;}
.f-list-content .right-content .job-ask{display:flex; display:-webkit-flex; justify-content:space-between;}
.f-list-content .right-content .job-ask .info,.f-list-content .right-content .job-ask .date{font-size:26rpx;}

/*搜索结果*/
.result-content .search-block .search-form{width:690rpx; position:relative;}
.result-content .search-block .search-form .total{position:absolute; right: 14rpx; top: 18rpx; color: #ddd; font-size:24rpx;}
.result-content .tab-head-group{height:88rpx; line-height:96rpx; border-bottom:1px solid #E5E5E5;position:fixed;top:112rpx;width:100%;background-color:#fff;}
.result-content .tab-head-group .tab-item{flex-grow:1; padding-left:30rpx; font-size: 28rpx;}
.result-content .tab-head-group .tab-item.z-active{color:#2284e5}
.result-content .tab-head-group .tab-item.z-active .u-icon-down{background-position:-336rpx -48rpx;}
.result-content .f-list-content .checkbox{float:left;}

.result-content .dropdown-block{position: relative; height: 0;top:200rpx;}
.result-content .dropdown-block .block-1,
.result-content .dropdown-block .block-2,
.result-content .dropdown-block .block-3,
.result-content .dropdown-block .block-4{position: absolute; width: 100%; background:#fff; z-index: 3000;top:0;}
.result-content .dropdown-block .block-2 .list-item{height: 70rpx; line-height: 70rpx; border-bottom:1px solid #E5E5E5; padding: 0 30rpx; color: #666;}
.result-content .dropdown-block .block-1 .title{padding-left: 38rpx; margin-top: 30rpx;}
.result-content .dropdown-block .block-1 .inner{padding-left: 25rpx; padding-right: 20rpx; flex-wrap:wrap; }
.result-content .dropdown-block .block-1 .name{flex-shrink:0; width: 128rpx; border:1px solid #E5E5E5; border-radius: 10rpx; margin: 10rpx; padding:0 12rpx;  text-align: center; font-size: 22rpx;}
.result-content .dropdown-block .block-1 .z-active{border:1px solid #e4442c; color: #e4442c;}/
.result-content .dropdown-block .block-1 .btn-area{margin-top:0}

.result-content .footer-btn{position: fixed; width: 100%; bottom: 0; height: 130rpx; z-index: 1000; background:#fff;border-top:1rpx solid #eee;}
.result-content .footer-btn .selected-total{float: left; width:160rpx; margin-left:30rpx; margin-top:40rpx;}
.result-content .footer-btn .btn-area{width:530rpx; float: left; margin-top:0;}
.result-content .footer-btn .btn-area .u-btn{width:470rpx;}

/*搜索*/
.search-page{background:#f1f1f1;}
.search-block{background:#efefef; height:95rpx; padding-top:25rpx; border-bottom:1px solid #E5E5E5;position:fixed;top:0;width:100%;}
.search-block .search-form{width:610rpx; height:70rpx; background:#fff; border-radius: 35rpx; float:left;}
.search-block .search-form .u-icon-32{float:left; margin:18rpx 10rpx 0 36rpx;}
.search-block .search-form .search-input{float:left; width: 530rpx; height: 70rpx; font-size:30rpx;}
.search-block .cancel{width:60rpx; height: 70rpx; line-height: 70rpx; text-align: center; float:right;}
.search-page .condition{background:#fff;}
.search-page .condition .item-list{display:flex; border-bottom:1px solid #E5E5E5; justify-content:space-between; padding: 30rpx;}
.search-page .condition .item-list .title{color:#999; width:200rpx;}
.search-page .condition .item-list .city{width:420rpx; text-align: right; margin-right:30rpx;}
.search-page .condition .item-list .u-icon-32{margin-top:6rpx;}
.search-page .condition .btn-area{padding:20rpx 30rpx; margin-top:0;}
.search-record{background:#fff;}
.search-record .title{border-bottom:1px solid #E5E5E5; padding: 30rpx;}
.search-record .list-item{display:flex; justify-content:space-between; padding: 30rpx; border-bottom:1px solid #E5E5E5;}
.search-record .list-item .name{width:600rpx;}
.search-record .list-item .u-icon-32{margin-top:6rpx;}

/*职位详情*/
.f-tab-group {margin-top:30rpx;}
.f-tab-group .tab-head-group{border-bottom:4rpx solid #2284e5; display: flex; justify-content: center;}
.f-tab-group .tab-head-group::after{content:"";overflow:hidden;clear:both;height:0;display: block;}
.f-tab-group .tab-head-group .tab-item{width:240rpx; height: 70rpx; background:url(https://wxapi.goodjobs.cn/images/tab.png) no-repeat 0 0; background-size:100% auto; text-align: center; line-height:70rpx; font-size:28rpx; position: relative; margin-left: -40rpx;}
.f-tab-group .tab-head-group .tab-item:first-child{margin-left:0;}
.f-tab-group .tab-head-group .tab-item.z-selected{ background:url(https://wxapi.goodjobs.cn/images/tab_on.png) no-repeat 0 0; background-size:100% auto; color: #fff; z-index: 2;}
.tab-main-content .tab-block .salary{color:#e4442c;}
.tab-main-content .tab-block .job-property .property-detail{margin-right:36rpx; padding:4rpx 0px}
.tab-main-content .tab-block .job-property .property-detail .u-icon-32{margin-right:4rpx;}
.tab-main-content .tab-block .job-welfare{justify-content:flex-start; flex-wrap:wrap; border-bottom:1rpx solid #E5E5E5;}
.tab-main-content .tab-block .job-welfare .welfare-txt{font-size:24rpx; border:1px solid #eee; border-radius:30rpx; overflow: hidden; height:32rpx; line-height: 34rpx; width: 106rpx; color: #666; margin-right: 16rpx; margin-bottom:8rpx; text-align:center; flex-shrink:0; background:#f0f0f0; padding:0 20rpx; }
.tab-main-content .tab-block .job-company{}
.tab-main-content .tab-block .job-company .list-item,
.tab-main-content .tab-block .publisher .list-item{justify-content:flex-start; padding: 4rpx 0;}
.tab-main-content .tab-block .job-company .list-item .gray,
.tab-main-content .tab-block .publisher .list-item .gray{color:#999; width:126rpx; flex-shrink:0; text-justify: auto; text-align:justify;}
.tab-main-content .tab-block .job-info .title,
.tab-main-content .tab-block .publisher .title{ border-bottom:1rpx solid #E5E5E5}
.tab-main-content .tab-block .job-info .over-hide{height: 165rpx;overflow: hidden;text-overflow : ellipsis;display:-webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp:4;}
.tab-main-content .tab-block .job-info .detail{text-align:justify;}
.tab-main-content .tab-block .publisher .list-item .gray{width:160rpx;}
.tab-main-content .tab-block .publisher .list-item .gray .u-icon-32{margin-right:4rpx;}

/*公司详情*/
.tab-main-content .company-block .job-company .list-item .gray{width:80rpx;}
.tab-main-content .company-block .company-logo .photo{width:690rpx; height: 300rpx;}
.tab-main-content .company-block .company-name{padding:30rpx 0; border-bottom:1px solid #E5E5E5;}
.company-gallery {width:690rpx; overflow: hidden;}
.company-gallery .title{border-bottom:1px solid #E5E5E5;}
.company-gallery .page-section-spacing{padding:30rpx 0; display:flex; flex-direction: row; flex-wrap: nowrap;}
.company-gallery .page-section-spacing .scroll-view-item_H{display: inline-flex; border-radius: 20rpx; margin-right:10rpx;}
.company-gallery .page-section-spacing .scroll-view-item_H image{width: 240rpx; height: 180rpx; border-radius: 20rpx;}
.company-job-list .list-item{padding:20rpx 0; border-bottom:1rpx  solid #eee;}
.company-job-list .list-item .title{font-size:32rpx; color: #333; margin-bottom:10rpx;}
.company-job-list .list-item .money{display:flex; display:-webkit-flex; justify-content:space-between; color: #999;}
.company-job-list .list-item .money .red{color:#ea4335}
.company-job-list .list-item .info{color:#999;}
.company-name-list{max-width:440rpx;overflow: hidden;text-overflow:ellipsis;white-space: nowrap;font-size:28rpx;padding-bottom:8rpx;color:#686868;}

/*关键词下拉框*/
.keywords-list{width: 100%;z-index: 999;background: #fff;position: absolute;
top:104rpx;left: 0;display: flex;flex-direction: column;border-left: 1rpx solid #eee;border-right: 1rpx solid #eee;}
.keywords-item{border-bottom: 1rpx solid #eee;padding:20rpx;}
.search-block .search-form .u-icon-32.search-icon-m {margin:9rpx 20rpx 1rpx 36rpx}

/*薪资选择*/
.salary-selector{background:url(https://wxapi.goodjobs.cn/images/select.png) no-repeat 0 0; width:50rpx;height:50rpx;float:right;margin-top:20rpx;}

.corp-name{display:flex;display:-webkit-flex;}
.uicon{display:inline-block;width: 50rpx;height: 24rpx;}
.recommend{background:url(https://wxapi.goodjobs.cn/images/tuijian.png) no-repeat;background-size:contain;margin-left: 12rpx;margin-top:10rpx;}
.is-top{background:url(https://wxapi.goodjobs.cn/images/ji.png) no-repeat;background-size:contain;margin-top:14rpx;margin-left:8rpx;}
